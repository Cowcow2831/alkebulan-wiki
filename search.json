---
layout: null
---
[
  {% for page in site.pages %}
    {% unless page.access_level == 'secret' %}
    {
      "title": {{ page.title | default: page.name | jsonify }},
      "url": "{{ site.baseurl }}{{ page.url }}",
      "content": {{ page.content | strip_html | strip_newlines | truncatewords: 50 | jsonify }},
      "access_level": "{{ page.access_level | default: 'public' }}"
    }{% unless forloop.last %},{% endunless %}
    {% endunless %}
  {% endfor %}
]
